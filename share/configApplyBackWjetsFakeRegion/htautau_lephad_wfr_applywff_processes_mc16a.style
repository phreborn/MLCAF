################################################



.name="QCDFakes",               .path="/mc16a/$(channel)/QCDFakes/[data/[data15*+data16*]]",  .isBackground=true,     histFillColor = kGray+1,        histFillStyle=1001,     title = "QCDFakes"
.name="WjetsFake",                 .path="/mc16a/$(channel)/WJETSFakes/data/[data15*+data16*]-/mc16a/$(channel)/WJETSFakes/mc/[Top+Ztautau+Diboson+Zee+Zmumu]-/mc16a/$(channel)/WJETSFakes/QCD/data/[data15*+data16*] ",       .isBackground=true,  histFillColor = kGray,  histFillStyle=1001,     title = "WjetsFake"



