# -*- mode: tqfolder -*-
# to get proper syntax highlighting and indentation when using emacs,
# add the following line to your .emacs file:
#   (load-file "$TQPATH/share/tqfolder.el")

+CutChannels {
  <.cutExpression = "$(fitsChannel)", .title="Channel Selection">
  +CutLeadLeptonPt {
         <.cutExpression = "[$(lep0).pt()] > 22000.", .title="$p_{t}^{lead} > 22$ GeV">
  }
  +CutMll {
         <.cutExpression = "[$(Mll)] < 55000.", .title="$M_{\ell\ell}<55$ GeV"> 
  }
  +Cut2PAODJets {
    <.cutExpression = "$(nPAODJets)>=2", .title="2-jet">
  }
} #End: CutChannels