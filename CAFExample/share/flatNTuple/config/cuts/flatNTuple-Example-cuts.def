# -*- mode: tqfolder -*-
# to get proper syntax highlighting and indentation when using emacs,
# add the following line to your .emacs file:
#   (load-file "$TQPATH/share/tqfolder.el")

+CutChannels {
    <.cutExpression = "$(fitsChannel)", .weightExpression = "[eventweight]", .title="Channel Selection">
    +CutLeptonsPt {
        <.cutExpression = " [ZcandLep0_pt] > 15000 && [ZcandLep1_pt] > 15000 && [fakecandLep_pt] > 15000", .title = "lep $p_T > 15$ GeV">
        +CutWZVeto {
            #<.cutExpression = "[PassWZVeto]", .title = "WZ veto">
            <.cutExpression = "[FakeLeptonMT] < 50000", .title = "WZ veto">


            +CutFakeEl {
                <.cutExpression = "[fakecandLep_type] == $(electron)", .title = "fake type: electron">
                +CutEtaFakeElec {
                    <.cutExpression = "(fabs([fakecandLep_eta]) < 2.47 && (fabs([fakecandLep_eta]) < 1.37 || fabs([fakecandLep_eta]) > 1.52))", .title = "Fake el eta cut">

                    +CutFakeElecID {
                        <.cutExpression = "[fakecandLep_ID]", .title = "ID cuts", flav_ID="El_ID">
                    }

                    +CutFakeElecAntiID {
                        <.cutExpression = "[fakecandLep_AntiID]", .title = "Anti-ID cuts", flav_ID="El_AntiID">
                    }

                }  #End: CutEtaFakeElec
            } #End: CutFakeEl


            +CutFakeMu {
                <.cutExpression = "[fakecandLep_type] == $(muon)", .title = "fake type: muon">
                +CutEtaFakeMuon {
                    <.cutExpression = "fabs([fakecandLep_eta]) < 2.5", .title = "Fake mu |eta|<2.5 ">

                    +CutFakeMuonID {
                        <.cutExpression = "[fakecandLep_ID]", .title = "ID cuts", flav_ID="Mu_ID">
                    }

                    +CutFakeMuonAntiID {
                        <.cutExpression = "[fakecandLep_AntiID]", .title = "Anti-ID cuts", flav_ID="Mu_ID">
                    }

                } #End: CutEtaFakeMuon
            } #End: CutFakeMu


        } #End: CutWZVeto
    } #End: CutLeptonsPt
} #End: CutChannels